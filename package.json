{
  "name": "imperium_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "generateBundleRelease:android": "cd android && ./gradlew bundleRelease",
    "generateApkRelease:android": "cd android && ./gradlew assembleRelease",
    "build:android": "cd android && ./gradlew build",
    "upload-symbols": "./ios/Pods/FirebaseCrashlytics/upload-symbols -gsp ./ios/GoogleService-Info.plist -p ios ../../Downloads/appDsyms",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.tsx --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ --reset-cache",
    "bundle:ios": "react-native bundle --entry-file='index.tsx' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios' --sourcemap-output='./ios/index.ios.js.map' --reset-cache",
    "clean": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "podinstall": "cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-native-firebase/analytics": "^15.4.0",
    "@react-native-firebase/app": "^15.4.0",
    "@react-native-firebase/crashlytics": "^15.4.0",
    "@react-native-firebase/remote-config": "^15.4.0",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "@types/semver": "^7.3.12",
    "axios": "^0.27.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "i18n-js": "^3.9.2",
    "intl": "^1.2.5",
    "lottie-ios": "^3.2.3",
    "lottie-react-native": "^5.0.1",
    "moment": "^2.29.3",
    "react": "17.0.2",
    "react-hook-form": "^7.30.0",
    "react-native": "0.68.1",
    "react-native-confirmation-code-field": "^7.3.0",
    "react-native-countdown-component": "^2.7.1",
    "react-native-countup": "0.0.2",
    "react-native-date-picker": "^4.2.1",
    "react-native-device-info": "^10.0.2",
    "react-native-dialog": "^9.2.2",
    "react-native-gesture-handler": "^2.4.1",
    "react-native-gifted-charts": "^1.2.29",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-notifier": "^1.7.1",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-tracking-transparency": "^0.1.1",
    "react-native-vector-icons": "^9.1.0",
    "react-native-wheel-color-picker": "^1.2.0",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "semver": "^7.3.7",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-native-countdown-component": "^2.7.0",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
